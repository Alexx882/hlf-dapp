{
	"name": "simplehlfnet",
	"version": "1.0.0",
	"client": {
		"tlsEnable": false,
		"adminUser": "admin",
		"adminPassword": "adminpw",
		"enableAuthentication": false,
		"organization": "SampleOrg",
		"connection": {
			"timeout": {
				"peer": {
					"endorser": "300"
				},
				"orderer": "300"
			}
		}
	},
	"channels": {
		"myc": {
			"peers": {
				"peer": {}
			},
			"connection": {
				"timeout": {
					"peer": {
						"endorser": "6000",
						"eventHub": "6000",
						"eventReg": "6000"
					}
				}
			}
		}
	},
	"organizations": {
		"SampleOrg": {
			"mspid": "SampleOrg",
			"fullpath": true,
			"adminPrivateKey": {
				"path": "/tmp/crypto/keystore/key.pem"
			},
			"signedCert": {
				"path": "/tmp/crypto/signcerts/peer.pem"
			}
		}
	},
	"peers": {
		"peer": {
			"url": "grpc://peer:7051",
			"eventUrl": "grpc://peer:7053"
		}
	}
}